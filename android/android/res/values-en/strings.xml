        [\&quot;(:Multiple author search:) $books[matches(author, &apos;Autor1|Autor2&apos;)]\&quot;,
         \&quot;(:Find books lend more than once:) for $book in $books group by $temp := $book.author || &apos;:&apos; || $book.title where count($book) &gt; 1 return $book[1]\&quot;,
         \&quot;(:Prim numbers:) for $i in 2 to 100 where empty((2 to $i - 1)[$i mod . = 0]) return $i\&quot;
        ]
    &quot; </string>
