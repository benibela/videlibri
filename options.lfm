        object templatexml: TMemo
          Left = 122
          Height = 158
          Top = 1
          Width = 480
          Anchors = [akTop, akLeft, akRight, akBottom]
          Lines.Strings = (
            '<!-- '
            ''
            'XML-Beschreibung eines Bibliotheksystems als Template.'
            ''
            'Schlagen Sie in der Hilfe nach, wie ein solches Template aufgebaut ist.'
            ''
            'Beispielstemplate:'
            '-->'
            ''
            '<actions>'
            '<action id="connect">'
            ' <!-- Aktion zum Herstellene einer Verbindung zum Bibliotheksserver -->'
            '  <variable name="db" value="DB={get(''picadb'', 1)}"/>'
            '  <variable name="loanServer" value="{$server}/loan/{$db}/LNG=DU/"/>'
            '</action>'
            ''
            '<action id="update-all">'
            ' <!-- Aktion zum Abfragen der aktuellen Ausleihen -->'
            '  <page url="{$loanServer}/USERINFO"> '
            '    <post name="ACT" value="UI_LOL"/>'
            '    <post name="BOR_U" value="{$username}"/>'
            '    <post name="BOR_PW" value="{$password}"/>'
            '    '
            '    <!-- Template zur Beschreibung der Ausleihliste -->'
            '    <template>'
            '    <html>     '
            '      <table summary="list of loans - data">'
            '        <template:read var="delete-current-books()"/>'
            '        <tr> <!-- Header der Liste -->'
            '          <td></td>'
            '          <td></td>'
            '          <td></td>'
            '          <td></td>'
            '          <td><!--title-->Titel</td>'
            '          <td></td>'
            '          <td></td>'
            '          <td></td>'
            '          <td><!--shelf mark-->Signatur</td>'
            '          <td></td>'
            '          <td></td>'
            '          <td></td>'
            '          <td><!--status-->Status</td>'
            '          <td></td>'
            '          <td></td>'
            '          <td></td>'
            '          <td><!--volume-->Band</td>'
            '          <td></td>'
            '          <td></td>'
            '          <td></td>'
            '          <td><!--expiry-->Leihfristende</td>'
            '          <td></td>'
            '          <td></td>'
            '          <td></td>'
            '          <td><!--reservations-->Vormerkungen</td>'
            '          <td></td>'
            '          <td></td>'
            '          <td></td>'
            '          <td><!--renewals-->Verlängerungen</td>'
            '          <td></td>'
            '          <td></td>'
            '          <td></td>'
            '        </tr>'
            '        '
            '        <tr> <td class="h2"></td> </tr>'
            '        '
            '        '
            '        '
            '        <!-- Abfragen aller Medien -->'
            '        <template:loop>'
            '        <tr>'
            '          {book := object()}'
            '          <td></td>'
            '          <td >'
            '            {book.extendId:=''''}'
            '            <input name="VB" type="checkbox">{book.extendId:=@value}</input>?'
            '          </td>'
            '          <td></td>'
            '          <td></td>'
            '          <td></td>'
            '          <td>'
            '            <template:read source="text()" var="book.title" regex="^[^/]*"/>'
            '            <template:read source="text()" var="book.author" regex="^[^/]*/\s*(von)?\s*(.*)" submatch="2"/>'
            '          </td>'
            '      '
            '          <td></td><td></td><td></td><td class="value-small">{$book.id}</td>'
            '          <td></td><td></td><td></td><td class="value-small">'
            '            <template:switch value="$book.extendId">'
            '              <template:if value="''''">'
            '                <template:switch value="filter(., ''Abholregal|Vormerkung|bestellt'')">'
            '                  <template:if value="''Abholregal''">'
            '                    {book.status := ., book.statusId := ''provided''}'
            '                  </template:if>'
            '                  <template:if value="''Vormerkung''">'
            '                    {book.status := ., book.statusId := ''ordered''}'
            '                  </template:if>'
            '                  <template:if value="''bestellt''">'
            '                    {book.status := ., book.statusId := ''ordered''}'
            '                  </template:if>'
            '                  <template:read var="book.status:problematic" source="."/>'
            '                </template:switch>'
            '              </template:if>'
            '              <template:read var="book.status:curious" source="."/>'
            '            </template:switch>'
            '          </td>'
            '          <td></td><td></td><td></td><td class="value-small"></td>'
            '          <td></td><td></td><td></td><td class="value-small">'
            '            <template:if test="text()!=''''"><template:read source="." var="book.duedate:dd-mm-yyyy"/></template:if>'
            '          </td>'
            '          <td></td><td></td><td></td><td class="value-small"></td>'
            '          <td></td><td></td><td></td><td class="value-small"><iframe></iframe></td>'
            '        </tr>'
            '      </template:loop>'
            '      '
            '      </table>'
            '    </html>'
            '    </template>'
            '    '
            '  </page>'
            '</action>'
            ''
            '<action id="renew-list">'
            '  <!-- Aktion zum Verlängern der Medien. '
            '       $renew-books ist die Liste der zu verlängernden Medien -->'
            '  <variable name="book-list">string-join(for $book in $renew-books return x"VB={$book.extendId}", "&amp;")</variable> '
            '  <page url="{$loanServer}/USERINFO" templateFile="extend">'
            '    <post name="ACT" value="UI_RENEWLOAN"/>'
            '    <post name="BOR_U" value="{$username}"/>'
            '    <post name="BOR_PW" value="{$password}"/>'
            '    <post value="{$book-list}"/>'
            '    <template>'
            '      <!-- Template zum Abfragen der neuen Ablauffristen -->'
            '      <table summary="list of loan renewals - data" template:optional="true">'
            '        <tr>'
            '        </tr>'
            '      '
            '        <template:loop>'
            '          <tr valign="top"><td>'
            '              <table summary="title data">'
            '                <tr><td class="plain"> <!--TITEL/AUTOR--></td></tr>'
            '                <tr><td>'
            '                  <span class="label-small">Signatur:<!--shelf mark--></span>'
            '                  {book:=object(("select(id)", filter(text(),''[^;]*'')))} '
            '                  <span class="label-small"><!--status:-->Status:</span>'
            '                  <template:read var="book.status" source="text()"/>'
            '                  <span class="label-small">Leihfristende:<!--expiry date:--></span>'
            '                  <template:read source="parse-date(text(), ''dd-mm-yyyy'')" var="book.duedate"/>'
            '                </td></tr>'
            '              </table>'
            '            </td>'
            '          </tr>'
            '        </template:loop>'
            '        '
            '      </table>'
            '    </template>'
            '    '
            '  </page>'
            '</action>'
            ''
            ''
            ''
            ''
            '<action id="search-connect">'
            '  <!-- Aktion für Suchverbindung -->'
            '  <call action="connect"/>'
            '</action>'
            ''
            '<action id="search">'
            '  <!-- Aktion für Suchstart -->'
            '</action>'
            ''
            '<action id="search-next-page">'
            '  <!-- Aktion für Suchnächste Seite -->'
            '</action>'
            ''
            ''
            '<action id="search-details">'
            '  <!-- Aktion für Suchdetails -->'
            '</action>'
            '</actions> '
          )
          ScrollBars = ssAutoBoth
          TabOrder = 0
